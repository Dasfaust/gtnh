<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
    <lines at="topleft">
        <line>
            <icon>
                <str>minecraft:sapling</str>
            </icon>
            <str> {lightpurple}{temperature}{white}°F ({lightpurple}</str>
            <round>
                <div>
                    <sub>
                        <var>temperature</var>
                        <num>32</num>
                    </sub>
                    <num>1.8</num>
                </div>
                <num>0</num>
            </round>
            <str>{white}°C, {lightpurple}</str>
            <round>
                <add>
                    <div>
                        <sub>
                            <var>temperature</var>
                            <num>32</num>
                        </sub>
                        <num>1.8</num>
                    </div>
                    <num>273.15</num>
                </add>
                <num>0</num>
            </round>
            <str>{white}K),</str>
            <str> {lightpurple}{humidity}{white}% humidity</str>
        </line>
        <line>
            <if>
                <equal>
                <var>gtnewore</var>
                    <str>true</str>
                </equal>
                <if>
                    <and>
                        <or>
                            <equal>
                                <modi>
                                    <var>chunkx</var>
                                    <num>3</num>
                                </modi>
                                <num>-2</num>
                            </equal>
                            <equal>
                                <modi>
                                    <var>chunkx</var>
                                    <num>3</num>
                                </modi>
                                <num>1</num>
                            </equal>
                        </or>
                        <or>
                            <equal>
                                <modi>
                                    <var>chunkz</var>
                                    <num>3</num>
                                </modi>
                                <num>-2</num>
                            </equal>
                            <equal>
                                <modi>
                                    <var>chunkz</var>
                                    <num>3</num>
                                </modi>
                                <num>1</num>
                            </equal>
                        </or>
                    </and>
                    <concat>
                        <icon>
                            <str>minecraft:diamond</str>
                        </icon>
                        <str> {cyan}Ore chunk{white}</str>
                    </concat>
                </if>
            </if>
            <if>
                <equal>
                    <var>gtnewore</var>
                    <str>false</str>
                </equal>
                <if>
                    <and>
                    <or>
                        <equal>
                            <modi>
                                <var>chunkx</var>
                                <num>3</num>
                            </modi>
                            <num>-1</num>
                        </equal>
                        <equal>
                            <modi>
                                <var>chunkx</var>
                                <num>3</num>
                            </modi>
                            <num>1</num>
                        </equal>
                    </or>
                    <or>
                        <equal>
                            <modi>
                                <var>chunkz</var>
                                <num>3</num>
                            </modi>
                            <num>-1</num>
                        </equal>
                        <equal>
                            <modi>
                                <var>chunkz</var>
                                <num>3</num>
                            </modi>
                            <num>1</num>
                        </equal>
                    </or>
                </and>
                    <concat>
                        <icon>
                            <str>minecraft:diamond</str>
                        </icon>
                        <str> {cyan}Ore chunk{white}</str>
                    </concat>
                </if>
            </if>
        </line>
        <line>
            <if>
                <and>
                    <equal>
                        <var>chunkoffsetx</var>
                        <num>7</num>
                    </equal>
                    <equal>
                        <var>chunkoffsetz</var>
                        <num>7</num>
                    </equal>
                </and>
                    <if>
                      <and>
                          <or>
                              <equal>
                                  <modi>
                                      <var>chunkx</var>
                                      <num>3</num>
                                  </modi>
                                  <num>-1</num>
                              </equal>
                              <equal>
                                  <modi>
                                      <var>chunkx</var>
                                      <num>3</num>
                                  </modi>
                                  <num>1</num>
                              </equal>
                          </or>
                          <or>
                              <equal>
                                  <modi>
                                      <var>chunkz</var>
                                      <num>3</num>
                                  </modi>
                                  <num>-1</num>
                              </equal>
                              <equal>
                                  <modi>
                                      <var>chunkz</var>
                                      <num>3</num>
                                  </modi>
                                  <num>1</num>
                              </equal>
                          </or>
                      </and>
                      <concat>
                          <icon>
                              <str>minecraft:compass</str>
                              <num>0</num>
                          </icon>
                          <str> {lightpurple}Chunk center{white}</str>
                      </concat>
                  </if>
            </if>
        </line>
    </lines>
</config>
